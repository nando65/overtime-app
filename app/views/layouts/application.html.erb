<!DOCTYPE html>
<html>
<head>
  <title>Overtime</title>

<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
<%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <%= render 'shared/nav' %>
<%= yield %>
     </div>
  </div>
</div>
<% if flash[:alert] %>
<%= js add_gritter(flash[:alert], title: "Overtime Alert", sticky: false) %>
<% elsif flash[:error] %>
<%= js add_gritter(flash[:error], title: "Overtime Error", sticky: false) %>
<% else %>
<%= js add_gritter(flash[:notice], title: "Overtime Notice", sticky: false) %>
<% end %>
</body>
</html>
